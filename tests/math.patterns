/* */

int_oper = { iadd | isub | imul | idiv | irem }
int_load = { ldc | iconst }

RULE: fold_const
int_load x
int_load y
int_oper : hi
-->
switch (hi) {
  iadd:
    ldc (x + y)
    ;
  isub:
    ldc (x - y)
    ;
  imul:
    ldc (x * y)
    ;
  idiv:
    ldc (x / y)
    ;
  irem:
    ldc (x % y)
    ;
}


RULE: pop_dup
dup
{istore | astore} : x
pop
-->
x


RULE: remove_nop
nop
[1] : x
-->
x

